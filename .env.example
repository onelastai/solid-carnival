# OneLastAI - Enterprise Environment Configuration
# Complete production-ready template with enterprise infrastructure
# Copy this file to .env and fill in your actual API keys and configuration

# =============================================================================
# CORE APPLICATION SETTINGS
# =============================================================================
RAILS_ENV=production
SECRET_KEY_BASE=your_secret_key_base_here
ENCRYPTION_KEY=your_encryption_key_here
APP_NAME=OneLastAI
APP_VERSION=1.0.0
APP_URL=https://yourdomain.com
API_VERSION=v1

# =============================================================================
# DATABASE CONFIGURATION - MONGODB ATLAS + POSTGRESQL
# =============================================================================
# MongoDB Atlas (Primary Database)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/onelastai?retryWrites=true&w=majority
MONGODB_DATABASE=onelastai_production
MONGODB_USERNAME=your_username
MONGODB_PASSWORD=your_password
MONGODB_CLUSTER=your_cluster_name
MONGODB_REPLICA_SET=atlas-replica-set
MONGODB_POOL_SIZE=20
MONGODB_TIMEOUT=10000

# PostgreSQL (Secondary/Analytics)
DATABASE_URL=postgresql://username:password@localhost:5432/onelastai_production
DB_POOL=25
DB_TIMEOUT=5000

# =============================================================================
# AWS EC2 INFRASTRUCTURE
# =============================================================================
# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_SESSION_TOKEN=your_aws_session_token

# EC2 Instance Configuration
EC2_INSTANCE_ID=i-1234567890abcdef0
EC2_INSTANCE_TYPE=c5.xlarge
EC2_KEY_PAIR_NAME=onelastai-keypair
EC2_SECURITY_GROUP=sg-1234567890abcdef0
EC2_SUBNET_ID=subnet-1234567890abcdef0

# Auto Scaling
AUTO_SCALING_GROUP=onelastai-asg
MIN_INSTANCES=2
MAX_INSTANCES=10
DESIRED_INSTANCES=3

# Load Balancer
ELB_NAME=onelastai-alb
ELB_DNS_NAME=onelastai-alb-123456789.us-east-1.elb.amazonaws.com

# =============================================================================
# AI MODEL API KEYS - EXTENDED ENTERPRISE SUITE
# =============================================================================

# OpenAI API Keys
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_ORGANIZATION_ID=org-your-organization-id-here

# Anthropic Claude API
ANTHROPIC_API_KEY=sk-ant-api-key-here

# Google AI / Gemini API
GOOGLE_AI_API_KEY=your-google-ai-api-key-here
GOOGLE_CLOUD_PROJECT_ID=your-project-id

# Hugging Face API
HUGGINGFACE_API_KEY=hf_your-huggingface-token-here

# Cohere API
COHERE_API_KEY=your-cohere-api-key-here

# Replicate API (for Stable Diffusion, etc.)
REPLICATE_API_TOKEN=r8_your-replicate-token-here

# =============================================================================
# SPECIALIZED AI SERVICES - MULTIMEDIA & CREATIVE
# =============================================================================

# Runway ML (Video Generation & AI Creative Tools)
RUNWAYML_API_KEY=your-runwayml-api-key-here
RUNWAYML_TEAM_ID=your-team-id-here
RUNWAYML_MODEL_VERSION=gen3

# ElevenLabs (Voice Synthesis)
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here

# AssemblyAI (Speech Recognition)
ASSEMBLYAI_API_KEY=your-assemblyai-api-key-here

# Stability AI (Image Generation)
STABILITY_API_KEY=sk-your-stability-ai-key-here

# DeepL (Translation)
DEEPL_API_KEY=your-deepl-api-key-here

# =============================================================================
# ENTERPRISE PAYMENT PROCESSING
# =============================================================================

# Stripe Payment Gateway
STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_CONNECT_CLIENT_ID=ca_your_connect_client_id

# PayPal Payment Gateway
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_ENVIRONMENT=live
PAYPAL_WEBHOOK_ID=your_webhook_id

# Lemon Squeezy Payment Platform
LEMONSQUEEZY_API_KEY=your_lemonsqueezy_api_key
LEMONSQUEEZY_STORE_ID=your_store_id
LEMONSQUEEZY_WEBHOOK_SECRET=your_webhook_secret

# =============================================================================
# PASSAGE AUTH (1PASSWORD POWERED AUTHENTICATION)
# =============================================================================

# Passage Authentication
PASSAGE_APP_ID=your_passage_app_id
PASSAGE_API_KEY=your_passage_api_key
PASSAGE_AUTH_STRATEGY=webauthn
PASSAGE_RSA_PUBLIC_KEY=your_rsa_public_key

# 1Password Integration
ONEPASSWORD_SECRET_KEY=your_1password_secret_key
ONEPASSWORD_SERVICE_ACCOUNT_TOKEN=your_service_account_token
ONEPASSWORD_VAULT_ID=your_vault_id

# =============================================================================
# CLOUD STORAGE & CDN - AWS S3 ENHANCED
# =============================================================================

# AWS S3 Configuration
S3_BUCKET_NAME=onelastai-storage
S3_REGION=us-west-2
S3_ACCESS_KEY_ID=your_s3_access_key
S3_SECRET_ACCESS_KEY=your_s3_secret_key
S3_CLOUDFRONT_DISTRIBUTION=your_cloudfront_distribution_id

# Cloudflare (CDN & Storage)
CLOUDFLARE_API_TOKEN=your-cloudflare-token-here
CLOUDFLARE_ZONE_ID=your-zone-id-here

# =============================================================================
# BACKUP & DISASTER RECOVERY
# =============================================================================

# MongoDB Atlas Backup
MONGODB_BACKUP_ENABLED=true
MONGODB_BACKUP_FREQUENCY=daily
MONGODB_BACKUP_RETENTION_DAYS=30

# AWS Backup
AWS_BACKUP_VAULT=onelastai-backup-vault
AWS_BACKUP_PLAN_ID=your_backup_plan_id
AWS_BACKUP_IAM_ROLE=arn:aws:iam::account:role/BackupRole

# =============================================================================
# ENHANCED DATABASE CONFIGURATION
# =============================================================================

# Redis Configuration (for caching and sessions)
REDIS_URL=redis://localhost:6379/0
REDIS_CACHE_URL=redis://localhost:6379/1
REDIS_SESSIONS_URL=redis://localhost:6379/2
REDIS_SIDEKIQ_URL=redis://localhost:6379/3

# =============================================================================
# ENTERPRISE MONITORING & ANALYTICS
# =============================================================================

# Application Performance Monitoring
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key
NEW_RELIC_APP_NAME=OneLastAI Production

# Error Tracking
SENTRY_DSN=https://your_sentry_dsn_here
SENTRY_ENVIRONMENT=production

# Analytics
GOOGLE_ANALYTICS_ID=GA-your-analytics-id
MIXPANEL_TOKEN=your_mixpanel_token
AMPLITUDE_API_KEY=your_amplitude_api_key

# =============================================================================
# DOCKER & CONTAINERIZATION
# =============================================================================

# Docker Configuration
DOCKER_REGISTRY=your-registry.com
DOCKER_IMAGE_TAG=latest
DOCKER_COMPOSE_PROJECT_NAME=onelastai

# Kubernetes
KUBE_NAMESPACE=onelastai-production
KUBE_CONFIG_PATH=/path/to/kubeconfig

# =============================================================================
# EMAIL & COMMUNICATION
# =============================================================================

# SendGrid (Email Service)
SENDGRID_API_KEY=SG.your-sendgrid-api-key-here
FROM_EMAIL=noreply@onelastai.com

# Twilio (SMS & Voice)
TWILIO_ACCOUNT_SID=ACyour-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Slack Integration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url

# Discord Bot
DISCORD_BOT_TOKEN=your-discord-bot-token-here

# =============================================================================
# ANALYTICS & MONITORING
# =============================================================================

# Google Analytics
GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX-X

# Mixpanel Analytics
MIXPANEL_TOKEN=your-mixpanel-token-here

# Sentry (Error Monitoring)
SENTRY_DSN=https://your-sentry-dsn-here

# New Relic (Performance Monitoring)
NEW_RELIC_LICENSE_KEY=your-new-relic-license-key

# =============================================================================
# PAYMENT PROCESSING
# =============================================================================

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# PayPal Configuration
PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_CLIENT_SECRET=your-paypal-client-secret
PAYPAL_MODE=sandbox

# =============================================================================
# SECURITY & AUTHENTICATION
# =============================================================================

# JWT Secret for API Authentication
JWT_SECRET=your-jwt-secret-key-here

# OAuth Configuration
GITHUB_CLIENT_ID=your-github-oauth-client-id
GITHUB_CLIENT_SECRET=your-github-oauth-client-secret

GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id
GOOGLE_OAUTH_CLIENT_SECRET=your-google-oauth-client-secret

# Session Configuration
SESSION_SECRET=your-session-secret-here

# =============================================================================
# RATE LIMITING & SECURITY
# =============================================================================

# Rate Limiting Configuration
RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_WINDOW_MS=900000

# CORS Configuration
CORS_ORIGIN=https://onelastai.com,https://www.onelastai.com

# =============================================================================
# DEVELOPMENT & TESTING
# =============================================================================

# Development Configuration
RAILS_LOG_LEVEL=debug
RAILS_SERVE_STATIC_FILES=true

# Test Environment
TEST_DATABASE_URL=sqlite3:db/test.sqlite3

# =============================================================================
# DEPLOYMENT CONFIGURATION
# =============================================================================

# Production Host Configuration
PRODUCTION_HOST=onelastai.com
API_BASE_URL=https://api.onelastai.com

# Docker Configuration
DOCKER_REGISTRY=registry.onelastai.com
DOCKER_IMAGE_TAG=latest

# Kubernetes Configuration
KUBERNETES_NAMESPACE=onelastai-production
KUBERNETES_CLUSTER=production-cluster

# =============================================================================
# FEATURE FLAGS
# =============================================================================

# Feature Toggles
ENABLE_PREMIUM_FEATURES=true
ENABLE_BETA_AGENTS=false
ENABLE_API_V2=true
MAINTENANCE_MODE=false

# =============================================================================
# NETWORKING & INFRASTRUCTURE
# =============================================================================

# Load Balancer Configuration
LOAD_BALANCER_IP=your-load-balancer-ip
HEALTH_CHECK_ENDPOINT=/health

# CDN Configuration
CDN_URL=https://cdn.onelastai.com

# =============================================================================
# BACKUP & RECOVERY
# =============================================================================

# Backup Configuration
BACKUP_S3_BUCKET=onelastai-backups
BACKUP_ENCRYPTION_KEY=your-backup-encryption-key

# =============================================================================
# THIRD-PARTY INTEGRATIONS
# =============================================================================

# GitHub Integration
GITHUB_WEBHOOK_SECRET=your-github-webhook-secret

# Zapier Integration
ZAPIER_API_KEY=your-zapier-api-key

# Make.com (formerly Integromat)
MAKE_API_KEY=your-make-api-key

# IFTTT Integration
IFTTT_SERVICE_KEY=your-ifttt-service-key